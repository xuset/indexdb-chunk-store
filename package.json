{
  "name": "indexdb-chunk-store",
  "version": "1.0.6",
  "description": "A Chunk Store compatible store backed by IndexDB",
  "main": "index.js",
  "scripts": {
    "build": "standard && browserify -r . -s IdbChunkStore | uglifyjs -m > idbchunkstore.min.js",
    "standard": "standard",
    "test": "standard && zuul -- test.js",
    "test-local": "standard && zuul --local -- test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xuset/indexdb-chunk-store.git"
  },
  "keywords": [
    "abstract-chunk-store",
    "chunk",
    "store",
    "indexdb",
    "idb"
  ],
  "author": "xuset",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/xuset/indexdb-chunk-store/issues"
  },
  "homepage": "https://github.com/xuset/indexdb-chunk-store#readme",
  "dependencies": {
    "idb-kv-store": "^4.3.0"
  },
  "devDependencies": {
    "abstract-chunk-store": "^1.2.1",
    "browserify": "^14.3.0",
    "standard": "^10.0.2",
    "tape": "^4.6.3",
    "uglify-js": "^2.7.4",
    "zuul": "^3.11.1"
  }
}
